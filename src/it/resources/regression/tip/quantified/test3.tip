(declare-datatypes (a)
  ((list (nil) (cons (head a) (tail (list a))))))

(define-fun-rec
  (par (a b)
    (map
       ((f (=> a b)) (xs (list a))) (list b)
       (match xs
         (case nil (as nil (list b)))
         (case (cons y ys) (cons (@ f y) (map f ys)))))))

(assert-not
  (par (a b c)
    (forall ((f (=> b c)) (g (=> a b)) (xs (list a)))
      (= (map (lambda ((x a)) (@ f (@ g x))) xs)
        (map f (map g xs))))))

(check-sat)
