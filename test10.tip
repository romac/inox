(declare-datatypes (a)
  ((list (nil) (cons (head a) (tail (list a))))))
(define-fun-rec
  (par (a)
    (append
       ((xs (list a)) (ys (list a))) (list a)
       (match xs
         (case nil (as nil (list a)))
         (case (cons x zs) (cons x (append zs ys)))))))
(assert-not
  (par (a)
    (forall ((xs (list a))) (= (append xs (as nil (list a))) xs))))
(check-sat)
